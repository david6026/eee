if _G.Misc = true then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Da Things (da hood)", "DarkTheme")

    local Tab = Window:NewTab("Main")
    local Section = Tab:NewSection("Very Shit Scripted but alr have fun (idc if leaked)")

    Section:NewButton("Headless (not perm)", "Bottom Text sex", function()
        game.Players.PlayerAdded:Connect(function(player)
            player.CharacterAdded:Connect(function(character)
                local head = character:WaitForChild("Head")
                head.Transparency = 1
        
                for _, item in head:GetChildren() do
                    if string.find (string.lower(item.Name), "face") then
                        item:Destroy()
                    end
                end
            end)
        end)
    end)
    
    local Main = Tab:NewSection("MISC")

    Section:NewSlider("Ping Spoofer (broken prob)", "Spoofs ur ping (client)", 10000, 1, function(s) -- 500 (MaxValue) | 0 (MinValue)
        local PerformanceStats = game:GetService("CoreGui"):WaitForChild("RobloxGui"):WaitForChild("PerformanceStats");
local PingLabel;
for I, Child in next, PerformanceStats:GetChildren() do
    if Child.StatsMiniTextPanelClass.TitleLabel.Text == "Ping" then
        PingLabel = Child.StatsMiniTextPanelClass.ValueLabel;
        break;
    end;
end;
 
local text = s
PingLabel:GetPropertyChangedSignal("Text"):Connect(function()
    PingLabel.Text = s
end);
PingLabel.Text = s
    end)

    Section:NewTextBox("IP Spoof", "put fake ip like 100.31.32.4", function(txt)
        --Creds to who ever made it lol
